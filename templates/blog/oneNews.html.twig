{% extends 'base.html.twig' %}
{% block title %}{{ title }}{% endblock %}
{% block body %}
    <div class="container">
        <article>
            <h1 class="text-dark">{{ new.title }}</h1>
            <div class="content">
                <img src="{{ new.image }}" alt="image">
                <br/>
                {{ new.content | raw}}
            </div>
        </article>
    </div>
{% endblock %}